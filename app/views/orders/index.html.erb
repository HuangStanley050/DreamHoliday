<h1>Orders#index</h1>
<h1>All available orders for user to select</h1>
<p>Find me in app/views/orders/index.html.erb</p>
<% @holidays.each do |holiday| %>
  <div>
    <h2><%= holiday.title %></h2>
    <p><%= holiday.description %></p>
    <h2><%= holiday.price %></h2>
    <%= image_tag(holiday.pictureUrl.service_url) %>
  </div>
  <button>Buy</button>
<% end %>
